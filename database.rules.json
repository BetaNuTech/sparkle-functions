{
  "rules": {
    "archive": {
      ".read": "auth != null",
      ".write": "auth != null",
      "propertyInspectionDeficientItems": {
        "$propertyID": {
          ".indexOn": ["item"]
        }
      }
    },
    "completedInspectionsList": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "inspections": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["property", "inspectionCompleted"]
    },
    "properties": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["code", "team"]
    },
    "propertyInspectionDeficientItems": {
      ".read": "auth != null",
      ".write": "auth != null",
      "$propertyID": {
        ".indexOn": ["item"]
      }
    },
    "propertyInspectionsList": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "propertyTemplatesList": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "registrationTokens": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "teams": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "templateCategories": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "templates": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["category"]
    },
    "templatesList": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["category"]
    },
    "users": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["email"]
    },
    "sendMessages": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "integrations": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "notifications": {
      ".read":"auth != null",
      ".write":"auth != null",
      "push": {
        ".indexOn": ["src"]
      }
    },
    "system": {
      "integrations": {
        "$uid": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        }
      }
    }
  }
}
