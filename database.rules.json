{
  "rules": {
    "users": {
      ".indexOn": [
        "email"
      ]
    },
    "inspections": {
      ".indexOn": [
        "property",
        "inspectionCompleted"
      ]
    },
    "properties": {
      ".indexOn": [
        "code"
      ]
    },
    "templates": {
      ".indexOn": [
        "category"
      ]
    },
    "templatesList": {
      ".indexOn": [
        "category"
      ]
    },
    "propertyInspectionDeficientItems": {
      "$propertyID": {
        ".indexOn": ["item"]
      }
    },
    ".read": "auth != null",
    ".write": "auth != null"
  }
}
