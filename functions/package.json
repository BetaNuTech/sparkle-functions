{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "test": "npm run test-unit && npm run test-int",
    "test-unit": "NODE_ENV=test LOG_LEVEL=crit npx mocha './{,!(node_modules)/**}/*.spec.js'",
    "unit-test": "npm run test-unit",
    "test-e2e": "NODE_ENV=test LOG_LEVEL=crit npx mocha './{,!(node_modules)/**}/test/end-to-end/**/*.test.js'",
    "end-to-end-test": "npm run test-e2e",
    "test-int": "NODE_ENV=test LOG_LEVEL=crit npx mocha './{,!(node_modules)/**}/test/integration/**/*.test.js'",
    "integration-test": "npm run test-int",
    "dev-test": "npx nodemon -i node_modules -e js,json --exec 'npm test --silent'",
    "dev": "npx nodemon -i node_modules -e js,json --exec 'node ./scripts/api-server.js'",
    "coverage": "npx nyc npm test --silent",
    "lint": "npx eslint --ignore-pattern '*/node_modules/*' --ext js,ts ."
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@google-cloud/error-reporting": "^2.0.1",
    "@google-cloud/pubsub": "^0.20.1",
    "app-root-path": "^2.2.1",
    "atob": "^2.0.3",
    "aws-sdk": "^2.94.0",
    "body-parser": "^1.18.3",
    "btoa": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.3.0",
    "got": "^9.6.0",
    "handlebars": "^4.1.2",
    "jimp": "0.2",
    "jsdom": "^11.1.0",
    "lodash": "^4.15.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.26",
    "navigator": "^1.0.1",
    "pdfmake": "^0.1.68",
    "swagger-ui-express": "^4.1.6",
    "triple-beam": "^1.3.0",
    "validate": "^5.1.0",
    "winston": "^3.1.0",
    "xml2js": "^0.4.23",
    "zipcode-to-timezone": "^0.0.9"
  },
  "private": true,
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "firebase-functions-test": "^0.1.7",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "prettier": "^1.18.2",
    "sinon": "^7.2.2",
    "supertest": "^3.4.2"
  }
}
